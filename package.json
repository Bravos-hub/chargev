{
  "name": "evzone-backend",
  "private": true,
  "version": "0.1.0",
  "description": "EVzone backend API",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:cov": "npx jest --coverage",
    "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npx jest --config ./apps/api/test/jest-e2e.json --runInBand",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:deploy": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.4.7",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.7",
    "@nestjs/platform-socket.io": "^10.4.7",
    "@nestjs/platform-ws": "^10.4.7",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/websockets": "^10.4.7",
    "@prisma/client": "^5.18.0",
    "@types/express": "^5.0.6",
    "@types/passport-local": "^1.0.38",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "fastify": "^4.28.1",
    "google-auth-library": "^10.5.0",
    "ioredis": "^5.4.1",
    "kafkajs": "^2.2.4",
    "mailgun.js": "^12.5.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "stripe": "^20.1.0",
    "twilio": "^5.11.1",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.4.7",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.16.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "jest": "^30.2.0",
    "prisma": "^5.18.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
